@{
    ViewBag.Title = "KPIs cá nhân";
    ViewBag.SubTitle = "KPIs chờ duyệt";
}
@section Menu {
    @Scripts.Render("~/Scripts/Tables/Data/menu")
}
@Styles.Render("~/BitexcoThemes/plugins/datatables/css")
<div class="row">
    <div class="col-xs-12">
        <div class="box">
            @*<div class="box-header">
                    <h3 class="box-title">Data Table With Full Features</h3>
                </div>*@
            <div class="box-body">
                <table id="ipf-pending-table" class="table table-bordered table-striped table-hover">
                    <thead>
                        <tr>
                            <th>Công ty</th>
                            <th>Phòng/bộ phận</th>
                            <th>Loại</th>
                            <th>Năm</th>
                            <th>Kỳ</th>
                            <th>Mã - Tên nhân viên</th>
                            <th>Chức vụ</th>
                            <th>Điểm công việc</th>
                            <th>Điểm năng lực</th>
                            <th>Tổng điểm</th>
                            <th>
                                <button type="button" class="btn btn-success btn-circle pull-right" onclick="@("window.location.href='" + @Url.Action("PendingCreate", "Ipf") + "'");"><i class="fa fa-plus-circle"></i></button>
                            </th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                    <tfoot>
                        <tr>
                            <th>Công ty</th>
                            <th>Phòng/bộ phận</th>
                            <th>Loại</th>
                            <th>Năm</th>
                            <th>Kỳ</th>
                            <th>Mã - Tên nhân viên</th>
                            <th>Chức vụ</th>
                            <th>Điểm công việc</th>
                            <th>Điểm năng lực</th>
                            <th>Tổng điểm</th>
                            <th></th>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/BitexcoThemes/plugins/datatables/js")
    @Scripts.Render("~/Scripts/Tables/Data")
    @Scripts.Render("~/Scripts/Ipf")
    @Scripts.Render("~/Scripts/Ipf/pending")
}
