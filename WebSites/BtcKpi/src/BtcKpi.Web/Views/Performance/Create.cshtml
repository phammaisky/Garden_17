@model BtcKpi.Web.ViewModels.PerformanceViewModel
@using BtcKpi.Model
@using BtcKpi.Service.Common
@{
    ViewBag.Title = "Hiệu suất công việc";
    if(Model.PerformanceLsfb.Id == 0)
    {
        ViewBag.SubTitle = "Thêm mới hiệu suất công việc";
    } else if(Model.PerformanceLsfb.Id != 0) {
        ViewBag.SubTitle = "Cập nhật hiệu suất công việc";
    }
}
@section Menu {
    @Scripts.Render("~/Scripts/Tables/Data/menu")
}

@Styles.Render("~/BitexcoThemes/plugins/datatables/css")

<div id="error-block" class="callout callout-danger" style="margin-bottom: 0 !important;">
    <h4><i class="fa fa-info"></i> Lỗi:</h4>
    <label id="ErrorMessage">@Html.DisplayFor(model => model.ErrorMessage)</label>
</div>
@using (Html.BeginForm())
{
        @Html.AntiForgeryToken()
        @Html.HiddenFor(model => Model.PerformanceLsfb.Id, new { @id = "performanceHiddenId" })
        @Html.HiddenFor(model => Model.FirstLoad)
        @Html.HiddenFor(model => Model.isApprove)
        @Html.HiddenFor(model => Model.PerformanceLsfb.ShowFormByProjType)
        <div class="box box-primary" id="info-block">
            <div class="box-body box-profile">
                <ul class="list-group list-group-unbordered">
                    <li class="list-group-item align-middle">
                        <div class="col-md-4 form-group">
                            <b class="col-md-4 control-label">Người duyệt : </b>
                            @Html.DropDownListFor(x => x.PerformanceLsfb.ApprovedBy, Model.Approves, "--- Chọn ---", new { @class = "col-md-8" })
                        </div>
                        <div class="col-md-4 form-group">
                            <b class="col-md-4 control-label">Dự án: </b>
                            @Html.DropDownListFor(x => x.PerformanceLsfb.ProjectId, Model.Projects, "--- Chọn ---", new {@onchange = "projectSearchChange(this.value)", @class = "col-md-8"})
                        </div>
                        <div class="col-md-4 form-group">
                            <b class="col-md-4 control-label">Loại hiệu suất: </b>
                            @Html.DropDownListFor(x => x.PerformanceLsfb.TypePerformanceId, Model.TypePerformances, "--- Chọn ---", new {@class = "col-md-8"})
                        </div>
                        <br/>
                    </li>
                    <li class="list-group-item align-middle">
                        <div class="col-md-4 form-group">
                            <b class="col-md-4 control-label">Loại : </b>
                            @Html.DropDownListFor(x => x.PerformanceLsfb.Type, Model.ScheduleTypes, "--- Chọn ---", new { @class = "col-md-8", @onchange = "scheduleChange(this.value)" })
                        </div>
                        <div class="col-md-4 form-group">
                            <b class="col-md-4 control-label">Năm : </b>
                            @Html.DropDownListFor(x => x.PerformanceLsfb.Year, Model.Years, "--- Chọn ---", new { @class = "col-md-8" })
                        </div>
                        <div class="col-md-4 form-group" id="per_ScheduleID">
                            <b class="col-md-4 control-label">Kỳ : </b>
                            @Html.DropDownListFor(x => x.PerformanceLsfb.Month, Model.Months, "--- Chọn ---", new { @class = "col-md-8" })
                        </div>
                        <div class="col-md-4 form-group" id="per_QuarterID">
                            <b class="col-md-4 control-label">Quý : </b>
                            @Html.DropDownListFor(x => x.PerformanceLsfb.QuarterId, Model.QuarterTypes, "--- Chọn ---", new { @class = "col-md-8" })
                        </div>
                        <br />
                    </li>
                    @if (@Model.isApprove == false)
                    {
                        <div class="col-md-12 text-right" style="margin-top: 10px;">
                            <button id="btnStart" type="button" class="btn btn-info btn-sm" title="Bắt đầu">Bắt đầu</button>
                            <button id="btnReset" type="button" class="btn btn-danger btn-sm" title="Hủy">Hủy</button>
                        </div>

                    }
                </ul>
            </div>
        </div>
        <div class="row" id="performance-block">
            <div class="col-md-12">
                <div class="nav-tabs-custom">
                    <div class="tab-content">
                        <div id="performance-manor-id">
                            <div class="box box-boder-top-1">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Leasing Extend - Office</h3>
                                    <div class="box-tools pull-right">
                                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                    </div>
                                </div>
                                <div class="box-body no-padding">
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Area</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.OfficeArea, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">$/m2/ month</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.OfficeMonthMoney, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                    <br />
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Rev TY/BG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.OfficeTY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">Rev BG/LY</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.OfficeLY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="box box-boder-top-1">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Leasing Extend - Retail</h3>
                                    <div class="box-tools pull-right">
                                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                    </div>
                                </div>
                                <div class="box-body no-padding">
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Area</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RetailArea, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">$/m2/ month</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RetailMonthMoney, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                    <br />
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Rev TY/BG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RetailTY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">Rev BG/LY</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RetailLY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                </div>
