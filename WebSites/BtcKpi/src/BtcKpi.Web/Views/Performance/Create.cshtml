                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">Revenue (TOS) with MG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RevAllTOSWithMG, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                    <br/>
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Revenue (TOS, RF, MF) no MG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RevAllNoMG, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">Revenue (TOS, RF, MF) with MG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RevAllWithMG, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Revenue (TOS, RF, MF) BD/LY</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RevAllLY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6" id="performance-fb-id">
                                            <label class="col-md-6"> $/m2/ month - OP</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RevAllOPMonthMoney, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="box box-boder-top-1">
                                <div class="box-header with-border">
