                                            <label class="col-md-6"> $/m2/ month</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.TotalMonthMoney, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">Total Rev TY/BG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.TotalRevTY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                    <br />
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Total Rev BG/LY</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.TotalRevLY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">Gross Profit TY/BG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.TotalGrossTY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                    <br />
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Gross Profit BG/LY</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.TotalGrossLY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class="col-12 form-group">
                                    <div class="col-md-12">
                                        <label class="col-md-3">Note</label>
                                        @Html.TextBoxFor(x => Model.PerformanceLsfb.Note, new { @class = "col-md-6" })
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                        <div id="performance-garden-fb-id">
                            <div class="box-body no-padding">
                                <div class="col-12 form-group">
                                    <div class="col-md-6">
                                        <label class="col-md-4">Type FB</label>
                                        @Html.DropDownListFor(x => Model.PerformanceLsfb.TypeFB, Model.TypeFBs, "", new {@class = "col-md-8", @onchange = "onChangeTypeFB(this.value)"})
                                    </div>
                                </div>
                                <br/>
                            </div>
                            <div class="box box-boder-top-1">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Sales</h3>
                                    <div class="box-tools pull-right">
                                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                    </div>
                                </div>
                                <div class="box-body no-padding">
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Line to line</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.SalesLineToLine, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">All</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.SalesAll, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                    <br/>
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Cash flow TY/BD</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.SalesCashFlowTY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">Cash flow BD/LY</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.SalesCashFlowLY, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="box box-boder-top-1">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Revenue line to line</h3>
                                    <div class="box-tools pull-right">
                                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                    </div>
                                </div>
                                <div class="box-body no-padding">
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Revenue (TOS) no MG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RevLineTOSNoMG, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">Revenue (TOS) with MG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RevLineTOSWithMG, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                    <br/>
                                    <div class="col-12 form-group">
                                        <div class="col-md-6">
                                            <label class="col-md-6">Revenue (TOS, RF, MF) no MG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RevLineNoMG, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-6">Revenue (TOS, RF, MF) with MG</label>
                                            @Html.TextBoxFor(x => Model.PerformanceLsfb.RevLineWithMG, new { @class = "col-md-6", @type = "number" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="box box-boder-top-1">
